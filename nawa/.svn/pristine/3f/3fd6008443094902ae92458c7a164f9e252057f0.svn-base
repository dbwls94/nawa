<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE HTML>
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
		<title>SignUp</title>
	</head>
	
	<body>
		<div id="Register">
			<h1>회원가입</h1>
			<form>

				<p><input type="email" id="inputUserEmail"  placeholder="email"/></p> 
				<p><input type="password" id="inputUserPwd"  placeholder="password"/></p>
			
			    <input type="text" id="inputUserName"  placeholder="�name"/>       

				<p><input type="text" id="inputUserEmail"  placeholder="가입할 이메일을 입력해주세요"/></p> 
				<p><input type="password" id="inputUserPwd"  placeholder="비밀번호를 입력해주세요"/></p>

				<select id="selectUserGender">   
					<option value="1">남자</option> 
					<option value="2">여자</option>
				</select>
				
				<img src="img/photo.jpg" class="profile-img" id="picture">
				<div class="picbutton">
				<input type="button" id="buttonPic" value="photo">

				    
				<p>
				<img src="img/photo.jpg" class="profile-img" style="width:120px;height:120px;">

				<input type="file" id="inputUserPic" >

				</div>
		
				<div class="okbtn">	
				<input type="button" id="btnUserOK" value="login" /> 
				<input type="button" id="btnUserCancel"  value="cancel" />
			   </div> 

				</p>
				<p><input type="text" id="inputUserName"  placeholder="이름을 입력해주세요"/> </p>   
				<p><input type="button" id="btnUserOK" value="확인" /></p> 
				<p><input type="button" id="btnUserCancel"  value="취소" /></p>

			</form>
		</div>
	   
		<!--  js library 파일  -->
